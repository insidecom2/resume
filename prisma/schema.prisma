datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}
 
generator client {
  provider = "prisma-client-js"
}

model User {
  id    Int     @id @default(autoincrement())
  full_name  String  
  email String  @unique
  password  String 
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model Notify {
  id    Int     @id @default(autoincrement())
  name  String  
  email String 
  subject String 
  message String 
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}